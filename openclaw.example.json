{
    "meta": {
        "lastTouchedVersion": "2026.2.3-1"
    },
    "wizard": {
        "lastRunVersion": "2026.2.3-1",
        "lastRunCommand": "doctor",
        "lastRunMode": "local"
    },
    "auth": {
        "profiles": {
            "google:default": {
                "provider": "google",
                "mode": "api_key"
            }
        }
    },
    "agents": {
        "defaults": {
            "model": {
                "primary": "google/gemini-2.5-flash"
            },
            "maxConcurrent": 4,
            "subagents": {
                "maxConcurrent": 8
            }
        },
        "list": [
            {
                "id": "main"
            },
            {
                "id": "trading",
                "model": {
                    "primary": "google/gemini-2.5-flash"
                },
                "identity": {
                    "name": "Trading Log Analyst"
                }
            }
        ]
    },
    "tools": {
        "web": {
            "search": {
                "enabled": true,
                "apiKey": "${WEB_SEARCH_API_KEY}"
            },
            "fetch": {
                "enabled": true
            }
        }
    },
    "messages": {
        "ackReactionScope": "group-mentions"
    },
    "commands": {
        "native": "auto",
        "nativeSkills": "auto",
        "restart": true
    },
    "channels": {
        "telegram": {
            "enabled": true,
            "dmPolicy": "allowlist",
            "botToken": "${TELEGRAM_BOT_TOKEN}",
            "allowFrom": [
                "6532799784"
            ],
            "groupPolicy": "allowlist",
            "streamMode": "partial"
        },
        "whatsapp": {
            "enabled": true,
            "qrCodePath": "C:\\Users\\user\\.openclaw\\whatsapp-qr.png",
            "dmPolicy": "allowlist",
            "allowFrom": [
                "${WHATSAPP_ALLOW_FROM}"
            ],
            "groupPolicy": "allowlist"
        }
    },
    "gateway": {
        "port": 18789,
        "mode": "local",
        "bind": "loopback",
        "auth": {
            "mode": "token",
            "token": "${GATEWAY_TOKEN}"
        },
        "tailscale": {
            "mode": "off",
            "resetOnExit": false
        }
    },
    "skills": {
        "install": {
            "nodeManager": "npm"
        },
        "entries": {
            "naver-calendar": {},
            "market-monitor": {},
            "goplaces": {
                "apiKey": "${GOPLACES_API_KEY}"
            },
            "local-places": {
                "apiKey": "${LOCAL_PLACES_API_KEY}"
            },
            "nano-banana-pro": {
                "apiKey": "${NANO_BANANA_PRO_API_KEY}"
            },
            "notion": {
                "apiKey": "${NOTION_API_KEY}"
            }
        }
    },
    "plugins": {
        "entries": {
            "telegram": {
                "enabled": true
            }
        }
    }
